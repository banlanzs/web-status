UPTIMEROBOT_API_KEY=urRO-XXXXXXXXXXXXXXXXXXXXXXXX

# 可选：前端自动刷新间隔（秒），默认 300
NEXT_PUBLIC_REFRESH_INTERVAL_SECONDS=300

# 可选：是否展示监控目标访问链接（true/false），默认 true
NEXT_PUBLIC_SHOW_MONITOR_LINKS=true

# 可选：启用自定义监控增强（当 UptimeRobot API 无响应时间数据时，直接检测目标 URL）
# 设置为 true 启用，false 或不设置则禁用
# 注意：启用后会直接从服务器向监控目标发送请求，可能增加服务器负载
NEXT_PUBLIC_USE_CUSTOM_MONITOR=false

